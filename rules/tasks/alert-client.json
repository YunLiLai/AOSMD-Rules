		"runOnStart": true,
		"interval": 300,
		"file": [
			"alert/tcp/bps/sip.rule",
			"alert/tcp/bps/sip-in.rule",
			"alert/tcp/bps/sip-out.rule",
			"alert/tcp/bps/sip-dip.rule",
			"alert/tcp/bps/sip-dip-in.rule",
			"alert/tcp/bps/sip-dip-out.rule",
			"alert/tcp/bps/sip-dip-dport.rule",
			"alert/tcp/bps/sip-dip-dport-in.rule",
			"alert/tcp/bps/sip-dip-dport-out.rule",
			"alert/tcp/bps/sip-dport.rule",
			"alert/tcp/bps/sip-dport-in.rule",
			"alert/tcp/bps/sip-dport-out.rule"
		]
	},

	"tcp-flows": {
		"enable": true,
		"runOnStart":true,
		"interval": 300,
