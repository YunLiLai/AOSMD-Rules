			"alert/tcp/flows/sip-dip-dport-syn.rule",
			"alert/tcp/flows/sip-dip-dport-synack.rule",
			"alert/tcp/flows/sip-dport-normal.rule",
			"alert/tcp/flows/sip-dport-rst.rule",
			"alert/tcp/flows/sip-dport-syn.rule",
