			"alert/tcp/flows/dip-dport-synack.rule",
			"alert/tcp/flows/sip-dip-dport-normal.rule",
			"alert/tcp/flows/sip-dip-dport-rst.rule",
			"alert/tcp/flows/sip-dip-dport-syn.rule",
			"alert/tcp/flows/sip-dip-dport-synack.rule"
		]
	},

	"tcp-latency": {
		"enable": true,
		"runOnStart":true,
		"interval": 300,
		"file": [
			"alert/tcp/latency/dip.rule",
			"alert/tcp/latency/dip-dport.rule",
			"alert/tcp/latency/sip-dip-dport.rule"
		]
	},

	"tcp-out-of-order": {
		"enable": true,
		"runOnStart":true,
		"interval": 300,
		"file": [
			"alert/tcp/out-of-order/dip.rule",
			"alert/tcp/out-of-order/dip-dport.rule",
			"alert/tcp/out-of-order/sip-dip-dport.rule"
		]
	},

	"tcp-retransmit": {
		"enable": true,
		"runOnStart":true,
		"interval": 300,
		"file": [
			"alert/tcp/retransmit/dip.rule",
			"alert/tcp/retransmit/dip-dport.rule",
			"alert/tcp/retransmit/sip-dip-dport.rule"
		]
	},

	"udp-bps": {
		"enable": true,
		"runOnStart":true,
		"interval": 300,
		"file": [
			"alert/udp/bps/total.rule"
		]
	},

	"http-flows": {
		"enable": true,
		"runOnStart":true,
		"interval": 300,
		"file": [
			"alert/http/flows/dip.rule",
			"alert/http/flows/url.rule",
			"alert/http/flows/sip-url.rule"
		]
