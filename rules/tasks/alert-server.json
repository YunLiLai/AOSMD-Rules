			"alert/http/status/sip-dip.rule",
			"alert/http/status/sip-url.rule",
			"alert/http/status/dip.rule",
			"alert/http/status/url.rule"
		]
	},

	"dns_flows": {
		"enable": true,
		"runOnStart": true,
		"interval": 300,
		"file": [
			"alert/dns/flows/dip.rule",
			"alert/dns/flows/domain.rule",
			"alert/dns/flows/total.rule"
		]
	},

	"dns_retcode": {
		"enable": true,
		"runOnStart": true,
		"interval": 300,
		"file": [
			"alert/dns/retcode/dip.rule",
			"alert/dns/retcode/domain.rule"
		]
	}
