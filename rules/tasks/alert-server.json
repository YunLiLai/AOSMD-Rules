{
	"itap": {
		"enable": true,
		"runOnStart": true,
		"interval": 300,
		"file": [
			"alert/itap/nodata-index.rule",
			"alert/itap/nodata-link.rule"
		]
	},

	"tcp-bps": {
		"enable": true,
		"runOnStart": true,
		"interval": 300,
		"file": [
			"alert/tcp/bps/dip.rule",
			"alert/tcp/bps/dip-in.rule",
			"alert/tcp/bps/dip-out.rule",
			"alert/tcp/bps/dip-dport.rule",
			"alert/tcp/bps/dip-dport-in.rule",
			"alert/tcp/bps/dip-dport-out.rule",
			"alert/tcp/bps/sip-dip-dport.rule",
			"alert/tcp/bps/sip-dip-dport-in.rule",
			"alert/tcp/bps/sip-dip-dport-out.rule"
		]
	},

	"tcp-flows": {
		"enable": true,
		"runOnStart":true,
		"interval": 300,
		"file": [
			"alert/tcp/flows/dip-normal.rule",
			"alert/tcp/flows/dip-rst.rule",
			"alert/tcp/flows/dip-syn.rule",
			"alert/tcp/flows/dip-synack.rule",
			"alert/tcp/flows/dip-dport-normal.rule",
			"alert/tcp/flows/dip-dport-rst.rule",
